{
    "name": "vigihdev/symfony-config-bridge",
    "description": "This package provides a configuration bridge for the Symfony Config component.",
    "homepage": "https://github.com/vigihdev/symfony-config-bridge",
    "keywords": [
        "symfony",
        "config",
        "bridge"
    ],
    "type": "library",
    "license": "MIT",
    "minimum-stability": "dev",
    "prefer-stable": true,
    "authors": [
        {
            "name": "Vigih Dev",
            "email": "vigihdev@gmail.com",
            "role": "developer"
        }
    ],
    "autoload": {
        "psr-4": {
            "Vigihdev\\Symfony\\ConfigBridge\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Vigihdev\\Symfony\\ConfigBridge\\Tests\\": "tests/"
        }
    },
    "config": {
        "process-timeout": 0
    },
    "require": {
        "php": "^8.1",
        "symfony/config": "^6.4",
        "symfony/dependency-injection": "^6.4",
        "symfony/yaml": "^6.4",
        "symfony/dotenv": "^6.4",
        "symfony/filesystem": "^6.4"
    },
    "require-dev": {
        "phpunit/phpunit": "10.5.x-dev"
    },
    "scripts": {
        "test": "phpunit",
        "dev": "php -S localhost:1392 local-dev/index.php"
    }
}